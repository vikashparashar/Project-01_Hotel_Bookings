{{template "base" .}}

{{define "css"}}
   <style>
        body {
            font-family: 'Righteous', cursive;
            background-color: #6D9886;
        }
    </style>
{{end}}



{{define "content"}}
     <div class="container">
        <div class="row">
            <div class="col text-center mt-5">
                <div id="carouselExampleCaptions" class="carousel slide carousel-fade"
                    data-bs-ride="false">
                    <div class="carousel-indicators">
                        <button type="button" data-bs-target="#carouselExampleCaptions"
                            data-bs-slide-to="0" class="active" aria-current="true"
                            aria-label="Slide 1"></button>
                        <button type="button" data-bs-target="#carouselExampleCaptions"
                            data-bs-slide-to="1" aria-label="Slide 2"></button>
                        <button type="button" data-bs-target="#carouselExampleCaptions"
                            data-bs-slide-to="2" aria-label="Slide 3"></button>
                    </div>
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img src="./images/10.jpg" class="d-block w-100" alt="room one"
                                height="600px">
                            <div class="carousel-caption d-none d-md-block">
                                <h5>We Have Rooms's Like - Duplex , Deluxe Room ,Studio Room </h5>
                                <p>
                                    A studio room offers all the amenities of a small apartment,
                                    plus a studio
                                    bed. This convertible bed can be turned into a sofa or another
                                    type of
                                    furniture. A studio room is ideal for longer-term stays.
                                </p>
                            </div>
                        </div>
                        <div class="carousel-item">
                            <img src="./images/07.jpg" class="d-block w-100" alt="room two"
                                height="600px">
                            <div class="carousel-caption d-none d-md-block">
                                <h5>General Suite</h5>
                                <p>A General suite comprises two rooms on separate floors, with a
                                    private
                                    staircase or lift between them. A duplex is a good choice for a
                                    guest such
                                    as the business traveller who wants to work or hold meetings in
                                    one room and
                                    sleep in the other.</p>
                            </div>
                        </div>
                        <div class="carousel-item">
                            <img src="./images/09.jpg" class="d-block w-100" alt="room three"
                                height="600px">
                            <div class="carousel-caption d-none d-md-block">
                                <h5>Major Suite</h5>
                                <p>A Major room will generally be larger and more lavishly appointed
                                    than
                                    other types of hotel rooms. They will often have additional
                                    features such as
                                    a whirlpool bath, a private patio or balcony, or a particularly
                                    outstanding
                                    view. The cost will also be higher .</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="row">
                        <div class="col">
                            <button class="carousel-control-prev" type="button"
                                data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                <span class="visually-hidden">Previous</span>
                            </button>
                        </div>
                        <div class="col">
                            <button class="carousel-control-next" type="button"
                                data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                <span class="visually-hidden">Next</span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{end}}



{{define "js"}}

{{end}}